        "# 4. Advanced Categorical Relationship Analysis\n",
        "\n",
        "# Prepare data for categorical analysis\n",
        "categorical_data = olist_data.dropna(subset=['primary_category', 'customer_state', 'value_category'])\n",
        "\n",
        "fig, axes = plt.subplots(2, 2, figsize=(18, 12))\n",
        "\n",
        "# Plot 1: Categorical scatter plot with jitter\n",
        "top_categories = categorical_data['primary_category'].value_counts().head(5).index\n",
        "cat_sample = categorical_data[categorical_data['primary_category'].isin(top_categories)].sample(n=500, random_state=42)\n",
        "\n",
        "sns.stripplot(data=cat_sample, x='primary_category', y='review_score', \n",
        "              size=4, alpha=0.7, ax=axes[0,0])\n",
        "axes[0,0].set_title('Review Scores by Product Category\\n(Strip Plot with Jitter)')\n",
        "axes[0,0].set_xlabel('Product Category')\n",
        "axes[0,0].set_ylabel('Review Score')\n",
        "axes[0,0].tick_params(axis='x', rotation=45)\n",
        "\n",
        "# Plot 2: Swarm plot (alternative to strip plot)\n",
        "# Use smaller sample for swarm plot performance\n",
        "swarm_sample = cat_sample.sample(n=200, random_state=42)\n",
        "sns.swarmplot(data=swarm_sample, x='primary_category', y='review_score', ax=axes[0,1])\n",
        "axes[0,1].set_title('Review Scores by Product Category\\n(Swarm Plot - No Overlap)')\n",
        "axes[0,1].set_xlabel('Product Category')\n",
        "axes[0,1].set_ylabel('Review Score')\n",
        "axes[0,1].tick_params(axis='x', rotation=45)\n",
        "\n",
        "# Plot 3: Point plot showing means with confidence intervals\n",
        "sns.pointplot(data=categorical_data, x='value_category', y='review_score', \n",
