      "source": [
        "# 1. Correlation Heatmaps for Business Intelligence\n",
        "\n",
        "# Prepare correlation data\n",
        "correlation_metrics = ['total_order_value', 'avg_item_price', 'item_count', \n",
        "                      'total_freight', 'freight_ratio', 'review_score']\n",
        "\n",
        "# Calculate correlation matrix\n",
        "correlation_data = olist_data[correlation_metrics].dropna()\n",
        "correlation_matrix = correlation_data.corr()\n",
        "\n",
        "# Create publication-quality heatmap\n",
        "plt.figure(figsize=(12, 8))\n",
        "\n",
        "# Create mask for upper triangle (optional - shows only lower triangle)\n",
        "mask = np.triu(np.ones_like(correlation_matrix, dtype=bool))\n",
        "\n",
        "# Generate heatmap\n",
        "sns.heatmap(correlation_matrix, \n",
        "            mask=mask,                    # Show only lower triangle\n",
        "            annot=True,                   # Show correlation values\n",
        "            fmt='.2f',                    # Format to 2 decimal places\n",
        "            cmap='RdBu_r',               # Red-Blue colormap (red=positive, blue=negative)\n",
        "            center=0,                     # Center colormap at 0\n",
        "            square=True,                  # Square cells\n",
        "            cbar_kws={'label': 'Correlation Coefficient'},\n",
        "            linewidths=0.5)               # Add gridlines\n",
        "\n",
        "plt.title('Olist Business Metrics Correlation Analysis\\nStrength of Relationships Between Key Variables', \n",
        "          fontsize=16, pad=20)\n",
        "plt.xlabel('')\n",
        "plt.ylabel('')\n",
        "plt.xticks(rotation=45, ha='right')\n",
        "plt.yticks(rotation=0)\n",
        "plt.tight_layout()\n",
        "plt.show()\n",
        "\n",
        "# Interpret correlations for business insights\n",
        "print(\"📊 Correlation Analysis Insights:\")\n",
        "print(\"\\n🔍 Strong Correlations (|r| > 0.7):\")\nstrong_corr = correlation_matrix[abs(correlation_matrix) > 0.7]\nfor i in range(len(strong_corr.columns)):\n    for j in range(i+1, len(strong_corr.columns)):\n        corr_val = strong_corr.iloc[i, j]\n        if abs(corr_val) > 0.7 and not np.isnan(corr_val):\n            print(f\"  • {strong_corr.columns[i]} ↔ {strong_corr.columns[j]}: {corr_val:.3f}\")\n\nprint(\"\\n⚠️ Business Interpretation:\")\nprint(\"• Positive correlation (r > 0): Variables increase together\")\nprint(\"• Negative correlation (r < 0): As one increases, other decreases\")\nprint(\"• |r| > 0.7: Strong relationship\")\nprint(\"• |r| 0.3-0.7: Moderate relationship\")\nprint(\"• |r| < 0.3: Weak relationship\")"
